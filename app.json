{
  "expo": {
    "name": "todo-list-app",
    "slug": "todo-list-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#778BDD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.dev.hyuk",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibaryUsageDescription" : "The app accesses ..."
      }
    },
    "android": {
      "package": "com.dev.hyuk",
      "versionCode": 1,
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_READ_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
